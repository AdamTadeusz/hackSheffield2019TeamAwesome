{
  "manifest_version": 2,
  "name": "Cookie Cutter",
  "version": "0.1",
    "browser_action": {
        "default_icon": "icons/icon-38.png"},
    "icons": { "16": "icons/icon-16.png",
        "19": "icons/icon-19.png",
        "38": "icons/icon-38.png",
        "48": "icons/icon-48.png",
       "128": "icons/icon-128.png" },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["scripts/jquery.js","scripts/script.js"]
    }
  ]
}